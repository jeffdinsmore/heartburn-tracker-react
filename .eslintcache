leEditFoodItemFormSubmission}>\r\n        <p className=\"pTagForm\">Food Name:</p>\r\n        <input className=\"field\"\r\n          type='text'\r\n          name='foodName'\r\n          placeholder='Food name'\r\n          defaultValue={foodItem.foodName}\r\n          required='required' />\r\n        <p className=\"pTagForm\">Brand:</p>\r\n        <input className=\"field\"\r\n          type='text'\r\n          name='brand'\r\n          placeholder='Name brand'\r\n          defaultValue={foodItem.brand}\r\n          required='required' />\r\n        <p className=\"pTagForm\">Ingredients:</p>\r\n        <textarea className=\"field2\"\r\n          type='text'\r\n          name='ingredients'\r\n          placeholder='Separate ingredients with commas'\r\n          defaultValue={foodItem.ingredients}\r\n          required='required' />\r\n        <p className=\"pTagForm\">Heartburn:</p>\r\n        <select name=\"heartburn\" className=\"field\" id=\"heartburnInput\" defaultValue={foodItem.heartburn}>\r\n          <option value=\"\" disabled>Please Select</option>\r\n          <option value=\"Yes\">Yes</option>\r\n          <option value=\"No\">No</option>\r\n        </select>\r\n        <br></br><br></br>\r\n        <button className=\"btn btn-success btn-sm\" type='submit'>Submit</button>\r\n      </form>\r\n    </React.Fragment>\r\n  );\r\n}\r\n\r\nEditFoodItemForm.propTypes = {\r\n  masterFoodItemList: PropTypes.object,\r\n  onEditFoodItem: PropTypes.func,\r\n};\r\n\r\nexport default EditFoodItemForm;","C:\\users\\Jeffrey\\documents\\epicodus\\react\\heartburn-tracker\\src\\components\\foodViews\\YourStats.js",["228"],"C:\\users\\Jeffrey\\documents\\epicodus\\react\\heartburn-tracker\\src\\App.js",["229","230","231","232","233"],"import React from 'react';\nimport Header from './components/layout/Header';\nimport FoodItemControl from './components/FoodItemControl';\nimport Footer from './components/Footer';\nimport Signin from \"./components/auth/Signin\";\nimport S